{
  "name": "fitlink-apps",
  "version": "0.0.2",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build -c nest-cli.serverless.json --webpack --webpackPath webpack-serverless.config.js",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start --watch",
    "start:jest": "cross-env DB_PASSWORD=jest DB_USERNAME=jest DB_DATABASE_NAME=jest DB_PORT=5433 PORT=3001 nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/api/main",
    "test": "jest --config ./apps/api/test/jest-e2e.config.js",
    "test:watch": "jest --config ./apps/api/test/jest-e2e.config.js --watch",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "db:diagram": "ts-node --project apps/api/tsconfig.uml.json ./node_modules/typeorm-uml/bin/run ./ormconfig.js --download=docs/uml.svg --format=svg --with-enum-values",
    "migration:show": "yarn run typeorm migration:show -f ./ormconfig.json",
    "migration:run": "yarn run typeorm migration:run -f ./ormconfig.js",
    "migration:revert": "yarn run typeorm migration:revert -f ./ormconfig.js",
    "migration:initialize": "yarn run typeorm migration:generate -n InitializeSchema -f ./ormconfig.js -d apps/api/database/migrations",
    "migration:generate": "yarn run typeorm migration:generate -f ./ormconfig.js -d apps/api/database/migrations",
    "migration:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "docs:generate": "@compodoc/compodoc -p tsconfig.json -s",
    "sls:offline:start": "cross-env API_PREFIX=\"api/v1\" && sls offline start",
    "sls:deploy": "serverless deploy",
    "sls:package": "serverless package",
    "sls:migrate": "serverless invoke -f migrate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.8.1",
    "@nestjs/common": "^7.6.13",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.13",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-fastify": "^7.6.13",
    "@nestjs/swagger": "^4.7.15",
    "@nestjs/typeorm": "^7.1.5",
    "aws-lambda": "^1.0.6",
    "aws-lambda-fastify": "^1.4.4",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.19.0",
    "fastify-multipart": "^4.0.2",
    "geojson": "^0.5.0",
    "lodash": "^4.17.21",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "serverless-dotenv-plugin": "^3.8.1",
    "sharp": "^0.27.2",
    "typeorm": "^0.2.31",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@databases/pg-test": "^3.0.0",
    "@hewmen/serverless-plugin-typescript": "^1.1.17",
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.13",
    "@types/bcrypt": "^3.0.0",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/sharp": "^0.27.1",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.4.0",
    "fastify-swagger": "^4.3.3",
    "jest": "^26.6.3",
    "mockdate": "^3.0.2",
    "prettier": "^2.2.1",
    "serverless-deployment-bucket": "^1.3.0",
    "serverless-domain-manager": "^5.1.0",
    "serverless-layers": "^2.3.3",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-optimize": "^4.1.4-rc.1",
    "serverless-plugin-typescript": "^1.1.9",
    "start-server-webpack-plugin": "2.2.5",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typeorm-uml": "^1.6.2",
    "typescript": "^4.1.5",
    "webpack": "5.24.2",
    "webpack-cli": "4.5.0",
    "webpack-node-externals": "2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
